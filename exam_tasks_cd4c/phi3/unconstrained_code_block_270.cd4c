
classdiagram FoodTraceabilitySystem {

    class Farmer {
      String uniqueIdentifier;
      String name;
      List<Product> harvestedProducts;
    }

    class Distributor {
      String uniqueIdentifier;
      String name;
      List<Product> purchasedProducts;
      void purchaseFromFarmer(Farmer farmer, Product product);
      void sellToRetailer(Retailer retailer, Product product);
    }

    class Retailer {
      String uniqueIdentifier;
      String name;
      List<Product> soldProducts;
      void purchaseFromDistributor(Distributor distributor, Product product);
      void sellToConsumer(Consumer consumer, Product product);
    }

    class Consumer {
      String uniqueIdentifier;
      String name;
      List<Product> purchasedProducts;
    }

    class Product {
      String uniqueIdentifier;
      String name;
      String type;
      String batchNumber;
      Date productionDate;
      String certifications;
      String blockchainTransaction;
    }

    class Blockchain {
      void recordTransaction(Product product);
      Product verifyProduct(String uniqueIdentifier);
    }

  composition Farmer -> (has) Product [*];
  composition Distributor -> (has) Product [*];
  composition Distributor -> (has) Retailer [*];
  composition Retailer -> (has) Consumer [*];
  composition Product -> (has) Blockchain [*];
}
