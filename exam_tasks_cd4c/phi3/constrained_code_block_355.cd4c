
classdiagram PodcastHostingPlatform {

  class PodcastHostingPlatform {
    List<Podcast> podcasts;
    List<Episode> episodes;
    List<Playlist> playlists;
    List<Comment> comments;
    List<Rating> ratings;
    PodcastAnalytics analytics;
  }

  class Podcast {
    String title;
    String description;
    String uniqueIdentifier;
    List<Genre> genres;
    List<Episode> episodes;
  }

  class Episode {
    String title;
    String description;
    String filePath;
  }

  class Playlist {
    String name;
    List<Podcast> podcasts;
  }

  class Comment {
    String content;
    String userId;
  }

  class Rating {
    int value;
    String userId;
  }

  class Genre {
    String name;
  }

  class PodcastAnalytics {
    int listeners;
    int downloads;
    int engagementMetrics;
  }

  association PodcastHostingPlatform -> List<<Podcast>>;
  association PodcastHostingPlatform -> List<<Episode>>;
  association PodcastHostingPlatform -> List<<Playlist>>;
  association PodcastHostingPlatform -> List<<Comment>>;
  association PodcastHostingPlatform -> List<<Rating>>;
  association PodcastHostingPlatform -> PodcastAnalytics;
}