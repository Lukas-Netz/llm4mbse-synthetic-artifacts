
classdiagram HeatMappingSystem {

    class HeatMapGenerator {
      void generateHeatMap(Sensor[] sensors);
      void storeHeatMap(HeatMap heatMap);
    }

    class Sensor {
      String uniqueId;
      double temperature;
      double humidity;
      double pressure;
    }

    class HeatMap {
      String identifier;
      double[][] data;
    }

    class GUI {
      void displayHeatMap(HeatMap heatMap);
      void displayMultipleHeatMaps(HeatMap[] heatMaps);
    }

    class Database {
      void storeHeatMap(HeatMap heatMap);
      HeatMap retrieveHeatMap(String identifier);
    }

    association HeatMapGenerator -> Sensor [*];
    association HeatMapGenerator -> GUI [*];
    association HeatMapGenerator -> Database [*];
    composition GUI -> HeatMap [*];
}