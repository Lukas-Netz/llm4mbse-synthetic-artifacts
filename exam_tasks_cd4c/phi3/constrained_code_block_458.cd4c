
classdiagram OnlinePsychologyPlatform {

    class Course {
      String title;
      String description;
      String courseId;
      CourseLevel level;
    }

    class Student {
      String studentId;
      List<Role> roles;
      List<Course> enrolledCourses;
    }

    class Assignment {
      String assignmentId;
      String title;
      String description;
      Date dueDate;
      Grade grade;
    }

    class DiscussionTopic {
      String topicId;
      String title;
      String description;
      List<Message> messages;
    }

    class Message {
      String authorId;
      String content;
      Date timestamp;
    }

    class Role {
      String roleId;
      String roleName;
    }

    class Grade {
      int score;
      String feedback;
    }

    class Instructor {
      String instructorId;
      List<Course> managedCourses;
      List<Assignment> managedAssignments;
      List<DiscussionTopic> managedTopics;
      StudentProgress studentProgress;
    }

    class TeachingAssistant {
      String teachingAssistantId;
      List<Course> managedCourses;
      List<Assignment> managedAssignments;
      List<DiscussionTopic> managedTopics;
    }

    class StudentProgress {
      Student student;
      List<Assignment> completedAssignments;
      List<DiscussionTopic> participatedTopics;
    }

    association Instructor -> Course [*];
    association Instructor -> Assignment [*];
    association Instructor -> DiscussionTopic [*];
    association TeachingAssistant -> Course [*];
    association TeachingAssistant -> Assignment [*];
    association TeachingAssistant -> DiscussionTopic [*];
    composition Student -> Role [*];
    composition Student -> Course [*];
    composition Student -> Assignment [*];
    composition Student -> DiscussionTopic [*];
   
 }