
classdiagram FashionPlatform {

    class User {
      String uniqueIdentifier;
      List<Profile> profiles;
    }

    class Profile {
      String userUniqueIdentifier;
      BodyMeasurements bodyMeasurements;
      FashionPreferences fashionPreferences;
      List<WardrobeItem> wardrobeItems;
    }

    class BodyMeasurements {
      float height;
      float weight;
      String bodyShape;
    }

    class FashionPreferences {
      String style;
      String colorPalette;
      List<String> preferredClothingItems;
    }

    class WardrobeItem {
      String uniqueIdentifier;
      String name;
      String description;
    }

    class ClothingItem extends WardrobeItem;
    class Accessory extends WardrobeItem;
    class Shoe extends WardrobeItem;

    class OutfitRecommendation {
      List<WardrobeItem> recommendedItems;
    }

    class OutfitReview {
      String userReview;
      int rating;
    }

    class ClothingDatabase {
      List<ClothingItem> clothingItems;
      List<Accessory> accessories;
      List<Shoe> shoes;
    }

    class MachineLearningService {
      void analyzeUserInput(User user);
      OutfitRecommendation recommendOutfit(User user);
    }

    composition User -> Profile [1..];
    composition Profile -> BodyMeasurements;
    composition Profile -> FashionPreferences;
    composition Profile -> WardrobeItem [*];
    composition User -> OutfitReview [*];
    composition User -> OutfitRecommendation [*];
    composition User -> ClothingDatabase;
    composition User -> MachineLearningService;
}
