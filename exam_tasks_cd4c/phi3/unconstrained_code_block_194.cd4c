
classdiagram MentalHealthPlatform {

    class User {
      String username;
      String password;
      String email;
      boolean loggedIn;
      List<Resource> accessedResources;
      List<Therapist> connectedTherapists;
      List<Goal> goals;
      List<Achievement> achievements;
      void createAccount();
      void login(String username, String password);
      void logout();
      void searchResourcesByTag(String tag);
      void bookSession(Therapist therapist);
      void trackProgress(String goal);
    }

    class Therapist {
      String therapistId;
      String username;
      String password;
      String qualifications;
      String experience;
      String availability;
      List<Resource> therapistResources;
      void createAccount();
      void login(String username, String password);
      void manageResources();
    }

    class Resource {
      String title;
      String description;
      String tags;
      int rating;
      String resourceType;
      void searchResourcesByTag(String tag);
    }

    class Goal {
      String goalId;
      String description;
      boolean isCompleted;
      void setGoal(String description);
      void trackProgress();
    }

    class Achievement {
      String achievementId;
      String description;
      boolean isCompleted;
      void setAchievement(String description);
      void trackProgress();
    }

    association User -> Resource [*];
    association User -> Goal [*];
    association User -> Achievement [*];
    association User -> Therapist [*];
    association Therapist -> Resource [*];
    association Therapist -> Goal [*];
    association Therapist -> Achievement [*];
}
