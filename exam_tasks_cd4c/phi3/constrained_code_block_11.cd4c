
classdiagram IndustrialRoboticsSystem {

  class Robot {
    String uniqueIdentifier;
    RobotStatus status;
  }

  abstract class Task {
    String name;
    String description;
    List<Step> steps;
  }

  class Step {
    String name;
    String description;
    List<Action> actions;
  }

  class Action {
    String name;
    String description;
    List<Parameter> parameters;
  }

  class Parameter {
    String name;
    String description;
    List<Value> values;
  }

  class Sensor {
    String name;
    String description;
    List<Reading> readings;
  }

  class Reading {
    String name;
    String description;
    List<Value> values;
  }

  class Actuator {
    String name;
    String description;
    List<Command> commands;
  }

  class Command {
    String name;
    String description;
    List<Parameter> parameters;
  }

  class Controller {
    String name;
    String description;
    List<Rule> rules;
  }

  class Rule {
    String name;
    String description;
    List<Condition> conditions;
  }

  class Condition {
    String name;
    String description;
    List<Value> values;
  }

  class User {
    String name;
    String description;
    List<Permission> permissions;
  }

  class Permission {
    String name;
    String description;
    List<Action> actions;
  }

  class Log {
    String name;
    String description;
    List<Entry> entries;
  }

  class Entry {
    String name;
    String description;
    List<Value> values;
  }

  class Report {
    String name;
    String description;
    List<Section> sections;
  }

  class Setting {
    String name;
    String description;
    List<Value> values;
  }

  class Notification {
    String name;
    String description;
    List<Value> values;
  }

  class Alert {
    String name;
    String description;
    List<Value> values;
  }

 
 
}