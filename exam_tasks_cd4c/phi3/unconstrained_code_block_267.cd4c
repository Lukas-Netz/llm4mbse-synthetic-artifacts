
classdiagram BlockchainArtSystem {

    class Artist {
      String uniqueIdentifier;
      String name;
      List<Artwork> artworks;
      void createArtwork(Artwork artwork);
    }

    class Artwork {
      String uniqueIdentifier;
      String title;
      String description;
      Artist artist;
      void verifyArtwork();
    }

    class Collector {
      String uniqueIdentifier;
      String name;
      List<Transaction> transactions;
      void purchaseArtwork(Artwork artwork, Transaction transaction);
    }

    class Transaction {
      Artist artist;
      Collector collector;
      Artwork artwork;
      int amount;
      void executeTransaction();
    }

    class Blockchain {
      List<Transaction> transactions;
      void addTransaction(Transaction transaction);
      void verifyTransaction(Transaction transaction);
      Artwork getArtwork(String uniqueIdentifier);
    }

    composition Artist -> (has) Artwork;
    composition Artist -> (has) Blockchain;
    composition Collector -> (has) Transaction;
    composition Collector -> (has) Blockchain;
    composition Transaction -> (has) Artwork;
    composition Transaction -> (has) Blockchain;
    composition Blockchain -> (has) Transaction;
}
