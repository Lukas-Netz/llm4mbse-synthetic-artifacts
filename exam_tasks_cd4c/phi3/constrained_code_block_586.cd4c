
classdiagram BioprintingSystem {

    class Printer {
      String model;
      String specifications;
      void print(Biomaterial biomaterial, PrintSpeed speed);
    }

    class Biomaterial {
      String name;
      String composition;
    }

    class PrintSpeed {
      int speed;
    }

    class PrintedTissue {
      String composition;
      String structure;
      String properties;
      void storeTissueInfo();
      void retrieveTissueInfo();
    }

    class ResearchAndDevelopment {
      void integrate(BioprintingSystem system);
    }

    class QualityControl {
      void inspect(PrintedTissue tissue);
    }

    class CustomerService {
      void handleInquiries(PrintedTissue tissue);
    }

    class BioprintingSystem {
      List<Printer> printers;
      List<PrintedTissue> tissues;
      ResearchAndDevelopment RnD;
      QualityControl QC;
      CustomerService CS;

      void addPrinter(Printer printer);
      void removePrinter(Printer printer);
      void printTissue(Printer printer, Biomaterial biomaterial, PrintSpeed speed);
      void storeTissueInfo(PrintedTissue tissue);
      void retrieveTissueInfo(PrintedTissue tissue);
      void integrateWithRnD();
      void inspectTissue(PrintedTissue tissue);
      void handleCustomerInquiry(PrintedTissue tissue);
    }
}