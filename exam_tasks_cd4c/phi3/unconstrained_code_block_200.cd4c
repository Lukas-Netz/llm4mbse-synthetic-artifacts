
classdiagram ClimateChangeSimulationSystem {

    class Simulation {
      String uniqueId;
      Date startDate;
    }

    class Scenario {
      String name;
      String description;
    }

    class Location {
      String name;
      double latitude;
      double longitude;
    }

    class WeatherStation {
      Location location;
      double temperature;
      double humidity;
      double windSpeed;
    }

    class SimulationRun {
      Simulation simulation;
      Scenario scenario;
    }

    class DataPoint {
      SimulationRun simulationRun;
      Location location;
      WeatherStation weatherStation;
    }

    composition Simulation -> SimulationRun;
    composition SimulationRun -> DataPoint;
    composition SimulationRun -> Scenario;
    composition SimulationRun -> Location;
    composition SimulationRun -> WeatherStation;
    composition SimulationRun -> Simulation;
    composition Scenario -> SimulationRun;
    composition Location -> WeatherStation;
    composition DataPoint -> SimulationRun;
    composition DataPoint -> WeatherStation;
}
