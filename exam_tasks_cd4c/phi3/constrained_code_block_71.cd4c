
classdiagram ElectionSystem {

    class VotingStation {
      List<Voter> voters;
      void castVote(Voter voter, Candidate candidate);
    cvvotingStation VotingStation;
    }

    class Voter {
      String voterId;
      void castVote(Candidate candidate);
      Voter();
    }

    class Candidate {
