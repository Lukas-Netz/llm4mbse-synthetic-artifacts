
classdiagram TsunamiWarningSystem {

    class Sensor {
      String sensorId;
      int waveIntensity;
      void detectTsunami();
    }

    class Buoy {
      String buoyId;
      List<Sensor> sensors;
      void receiveAlert(Sensor sensor);
      void transmitAlert(WarningCenter warningCenter);
    }

    class WarningCenter {
      String centerId;
      List<Buoy> buoys;
      List<Community> communities;
      void analyzeData();
      void issueWarning(Community community);
    }

    class Community {
      String name;
      String location;
    }

    class HistoricalData {
      String dataId;
      String data;
    }

    class UserInterface {
      void monitorSystem();
      void issueFalseAlarm(Community community);
    }

    composition Sensor -> (has) Buoy;
    composition Buoy -> (has) WarningCenter [*];
    composition WarningCenter -> (has) Community [*];
    composition WarningCenter -> (has) HistoricalData;
    composition WarningCenter -> (has) UserInterface;
}
