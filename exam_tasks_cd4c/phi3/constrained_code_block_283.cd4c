
classdiagram OnlineLearningPortal {

    class OnlineLearningPortal {
      Course course;
      Instructor instructor;
      Student student;
      Section section;
    Writes reports;
      List<Student> enrolledStudents;
      List<Course> availableCourses;
      Map<Instructor, List<Course>> instructorCourses;
    }

    class Course {
      String title;
      String description;
      String courseId;
      List<Section> sections;
      Instructor instructor;
    }

    class Instructor {
      String name;
      String email;
      String instructorId;
      List<Course> taughtCourses;
    }

    class Student {
      String name;
      String email;
      String studentId;
      List<Section> enrolledSections;
    }

    class Section {
      String sectionId;
      Date startDate;
      Date endDate;
      int maxCapacity;
      Course course;
      List<Student> enrolledStudents;
    }

    association Course 
      hasInstructor [many]
      -> Section [*];

    association Instructor 
      hasTaughtCourses [many]
      -> Course;

    association Student 
      hasEnrolledSections [many]
      -> Section;

    association Course 
      hasAvailableCourses [many]
      -> OnlineLearningPortal;

    association OnlineLearningPortal 
      hasEnrolledStudents [many]
      -> Student;

    association OnlineLearningPortal 
      hasAvailableCourses [many]
      -> Course;

    association OnlineLearningPortal 
      hasInstructorCourses [many]
      -> Instructor;
}