
classdiagram PersonalizedDNAAnalysis {

    class User {
      String uniqueIdentifier;
      DNAData userDNAData;
      List<Recommendation> personalizedRecommendations;
      void uploadDNAData();
      void viewRecommendations();
    }

    class DNAData {
      String userID;
      String geneticInformation;
    }

    class Recommendation {
      String recommendationType;
      String associatedUser;
    }

    class HealthRecommendation extends Recommendation {
      String healthCondition;
      String advice;
    }

    class WellnessRecommendation extends Recommendation {
      String wellnessMetric;
      String advice;
    }

    class LifestyleRecommendation extends Recommendation {
      String lifestyleHabit;
      String advice;
    }

    class Expert {
      String expertiseArea;
      String advice;
    }

    composition User -> DNAData;
    composition User -> Recommendation [*];
    association Recommendation -> User [*];
    association DNAData -> User [*];
    composition Expert -> HealthRecommendation;
    composition Expert -> WellnessRecommendation;
    composition Expert -> LifestyleRecommendation;
}
