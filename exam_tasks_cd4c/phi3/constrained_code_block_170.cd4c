
classdiagram ConsumerInsightsPlatform {

    class DataWarehouse {
      void importData(DataSource source);
      DataSet cleanedData;
    WritableDataSet transformData(DataSet data);
      DataSet aggregateData(DataSet data);
    }

    class DataProcessingModule {
      void cleanData(DataSet data);
      void transformData(DataSet data);
      void aggregateData(DataSet data);
    }

    class VisualizationModule {
      void createDashboard(DataSet data);
      void createReport(DataSet data);
    }

    class MachineLearningModule {
      void performClustering(DataSet data);
      void performRegression(DataSet data);
      void performDecisionTree(DataSet data);
    }

    class UserInterface {
      void displayDashboard(DataSet data);
      void displayReport(DataSet data);
      void filterData(DataSet data, String criteria);
      void sortData(DataSet data, String sortBy);
    }

    class DataSource {
      String sourceType;
    }

    class DataSet {
      List<Customer> customers;
      List<Transaction> transactions;
    }

    class Customer {
      String demographics;
      List<Purchase> purchases;
    }

    class Transaction {
      String transactionId;
      Date transactionDate;
      double amount;
    }

    class Purchase {
      String product;
      double price;
      Date purchaseDate;
    }
}