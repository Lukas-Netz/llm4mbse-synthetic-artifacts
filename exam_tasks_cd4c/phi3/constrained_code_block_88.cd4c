
classdiagram MachineVisionSystem {

    class Camera {
      String uniqueIdentifier;
      CameraType type;
    }

    class Lens {
      double focalLength;
      List<Camera> cameras;
    }

    class ProcessingUnit {
      double processingPower;
      List<Camera> cameras;
      void processImages();
      void classifyProduct();
    }

    class Database {
      Product product;
      void storeProductClassification(Product product);
    }

    enum CameraType {
      RGB, Thermal
   
   
   ;}

    enum ProcessingTask {
      ObjectDetection, Tracking, Classification
   
   
   ;}

    class Product {
      String name;
      String description;
      String classification;
    }

    composition Camera -> Lens [*];
    composition Camera -> ProcessingUnit [*];
    composition ProcessingUnit -> Database [*];
    composition Lens -> Camera [*];
    composition Lens -> ProcessingUnit [*];
}