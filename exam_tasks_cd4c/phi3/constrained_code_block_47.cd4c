
classdiagram ATMSystem {

    class ATM {
      String uniqueIdentifier;
      BankAccount account;
      CashDispenser cashDispenser;
      void depositCash(int amount);
      void withdrawCash(int amount);
      int checkBalance();
    }

    class BankAccount {
      String accountNumber;
      int balance;
      void deposit(int amount);
      void withdraw(int amount);
    }

    class CashDispenser {
      int capacity;
      int currentCash;
      void dispenseCash(int amount);
    }

    class SecuritySystem {
      void trackTransactions();
      void alertBank();
    }

    association ATM -> BankAccount;
    association ATM -> CashDispenser;
    composition ATM -> SecuritySystem;
}