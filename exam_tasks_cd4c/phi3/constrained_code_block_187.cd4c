
classdiagram DigitalPathologySystem {

    class Image {
      String uniqueIdentifier;
      String patientInfo;
      String diagnosis;
    }

    class User {
      String userId;
      String role;
      boolean isLoggedIn;
      Set<Permission> permissions;
    }

    class Permission {
      String name;
    }

    class Viewer {
      int zoomLevel;
      Point focusPoint;
      void displayImage(Image image);
    }

    class Analysis {
      List<Algorithm> algorithms;
      Image image;
      void analyzeImage();
    }

    class Algorithm {
      String name;
      int parameters;
      void apply(Image image);
    }

    class Diagnosis {
      double confidenceLevel;
      String diagnosisType;
      void generateDiagnosis(Image image);
    }

    class Report {
      String format;
      String content;
      void generateReport(Diagnosis diagnosis);
    }

    class DigitalPathologySystem {
      Database imageDatabase;
      Viewer viewer;
      Analysis analysis;
      Diagnosis diagnosis;
      Report report;
      Set<User> users;
    }

  association DigitalPathologySystem -> ImageDatabase [*];
  association DigitalPathologySystem -> Viewer;
  association DigitalPathologySystem -> Analysis;
  association DigitalPathologySystem -> Diagnosis;
  association DigitalPathologySystem -> Report;
  association DigitalPathologySystem -> User [*];
}