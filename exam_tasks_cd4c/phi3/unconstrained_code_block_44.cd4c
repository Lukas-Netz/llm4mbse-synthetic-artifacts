
classdiagram FinancialSystem {

    class FinancialSystem {
      AccountManager accountManager;
      ReportGenerator reportGenerator;
    }

    class AccountManager {
      Account account;
      void addAccount(Account account);
      void removeAccount(Account account);
      Account getAccount(String accountNumber);
      void processTransaction(Transaction transaction);
      List<Transaction> getTransactions(String accountNumber);
    }

    class Account {
      String accountNumber;
      double balance;
      List<Transaction> transactions;
    }

    class Transaction {
      String type;
      double amount;
      String fromAccount;
      String toAccount;
    }

    class ReportGenerator {
      void generateAccountReport(String accountNumber);
      void generateSummaryReport();
    }

    association FinancialSystem -> AccountManager;
    association FinancialSystem -> ReportGenerator;
    association AccountManager -> Account;
    association AccountManager -> Transaction;
    association ReportGenerator -> Account;
}
