

classdiagram VotingSystem {

  class Voter {
    String uniqueIdentifier;
    PublicKey publicKey;
  }

  class Candidate {
    String uniqueIdentifier;
    String name;
  }

  class Election {
    String uniqueIdentifier;
    Date startDate;
    Date endDate;
  }

  class Ballot {
    String electionId;
    List<Vote> votes;
  }

  class Vote {
    String ballotId;
    Candidate candidate;
  }

  class Block {
    List<Vote> votes;
    Hash previousBlockHash;
  }

  composition Voter -> (has) PublicKey;
  composition Election -> (has) Ballot [0..*];
  composition Ballot -> (has) List<<Vote>>;
  composition Block -> (has) List<<Vote>>;
  association Block -> PreviousBlock;
}