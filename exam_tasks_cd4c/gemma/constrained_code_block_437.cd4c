

classdiagram BioprintingSystem {

  class Printer {
    String name;
    int capacity;
    int speed;
    List<Biomaterial> biomaterials;

    Printer(String name, int capacity, int speed);
    void print(Biomaterial biomaterial, int quantity);
  }

  class Biomaterial {
    String name;
    Composition composition;
    Structure structure;
    Properties properties;

    Biomaterial(String name, Composition composition, Structure structure, Properties properties);
  }

  class PrintingProcess {
    String type;
    int speed;
    Biomaterial biomaterial;
    int quantity;

    PrintingProcess(String type, int speed, Biomaterial biomaterial, int quantity);
  }

  class Tissue {
    String name;
    Composition composition;
    Structure structure;
    Properties properties;

    Tissue(String name, Composition composition, Structure structure, Properties properties);
  }

  class Database {
    Map<Tissue, List<PrintingProcess>> tissueToPrintingProcesses;
    Map<Printer, List<Biomaterial>> printerToBiomaterials;

    void storeTissue(Tissue tissue);
    void storePrintingProcess(PrintingProcess printingProcess);
    void retrieveTissue(Tissue tissue);
    void retrievePrintingProcess(PrintingProcess printingProcess);
  }

  class Integration {
    void integrateWithResearchAndDevelopment();
    void integrateWithQualityControl();
    void integrateWithCustomerService();
  }

  composition BioprintingSystem -> (has) Database;
  composition BioprintingSystem -> (has) Integration;
}