

classdiagram PhotoEditingPlatform {

  class UserAccount {
    String username;
    String password;
    List<Photo> photos;
  }

  class Photo {
    String title;
    String description;
    String uniqueIdentifier;
    String format;
    List<String> tags;
  }

  class Filter {
    String name;
    int value;
  }

  class Effect {
    String name;
    String description;
  }

  class SocialMediaPlatform {
    void sharePhoto(Photo photo);
  }

  composition UserAccount -> (has) Photos;
  composition Photo -> (has) Tags;
  association Photo -> Filter;
  association Photo -> Effect;
  association SocialMediaPlatform -> UserAccount;
}