

classdiagram DevOpsToolchain {

  class ProjectManager {
    List<Project> projects;
  }

  class Project {
    String name;
    String description;
    List<Task> tasks;
  }

  class Task {
    String name;
    String description;
    TaskStatus status;
    TeamMember assignedTo;
  }

  class TeamMember {
    String name;
    String email;
    List<Project> projects;
  }

  class VersionControlSystem {
    List<Repository> repositories;
  }

  class Repository {
    String name;
    List<File> files;
  }

  class File {
    String name;
    String content;
  }

  class TaskStatus {
    String name;
  }

  class CI_CDSystem {
    BuildTool buildTool;
    TestingFramework testingFramework;
  }

  class BuildTool {
    String name;
  }

  class TestingFramework {
    String name;
  }

  composition ProjectManager -> (has) Project [*];
  composition Project -> (has) Task [*];
  composition Task -> (has) TeamMember;
  composition VersionControlSystem -> (has) Repository [*];
  composition Repository -> (has) File [*];
  composition CI_CDSystem -> (has) BuildTool;
  composition CI_CDSystem -> (has) TestingFramework;
}