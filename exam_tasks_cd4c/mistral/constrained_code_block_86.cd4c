
classdiagram SmartManufacturing {

  class ProductionLine {
    String name;
    String uniqueIdentifier;
    Machine [  ] machines;
  }

  abstract class Machine {
    String uniqueIdentifier;
  }

  class LaserCutter extends Machine {
    int cuttingSpeed;
    int precisionLevel;
  }

  class DrillingMachine extends Machine {
    int drillingSpeed;
    int torque;
  }

  class AssemblyRobot extends Machine {
    int assemblySpeed;
    int accuracy;
  }

  class QualityControl {
    Inspector [ ] inspectors;
  }

  abstract class Inspector {
    String uniqueIdentifier;
    Skill [ ] skills;
  }

  enum Skill {
    VisualInspection,
    MechanicalInspection,
    ChemicalAnalysis;
  }

  class Reporting {
    ProductionLine [ ] productionLines;
    QualityControl qualityControl;
  }

  composition ProductionLine -> Machine [*];
  composition QualityControl -> Inspector [*];
}