
classdiagram ConsumerDataManagement {

  class Consumer {
    String uniqueIdentifier;
    String name;
    Preferences preferences;
    Interaction [  ] interactions;
  }

  class Preferences {
    FavoriteProducts favoriteProducts;
    CommunicationChannels preferredCommunicationChannels;
    Language preferredLanguage;
  }

  class FavoriteProducts {
    Product [  ] products;
  }

  class CommunicationChannels {
    Channel [  ] channels;
  }

  enum Channel {
    Phone, Email, SocialMedia;
  }

