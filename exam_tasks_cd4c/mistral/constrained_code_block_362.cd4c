
classdiagram JobPortal {

  class JobSeeker {
    String name;
    String email;
    String password;
    JobApplication [ ] applications;
  }

  class JobOpening {
    String title;
    String description;
    String requirements;
    Company company;
    JobApplication [ ] applications;
  }

  class Company {
    String name;
    JobOpening [ ] jobOpenings;
  }

  class JobApplication {
    JobOpening jobOpening;
    JobSeeker jobSeeker;
  }

  association JobSeeker --o000o- 											;
  association JobOpening --o000o-											;
  association Company --o000o-												;
  association JobOpening --o1- 											;
  association JobSeeker --o1-												;
  association JobApplication --o1-											;
}