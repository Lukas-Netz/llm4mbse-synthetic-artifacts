
classdiagram EcommerceMarketplace {

  class Seller {
    String uniqueIdentifier;
    String name;
    Product [][] offeredProducts;
    Warehouse [][] warehouses;
  }

  class Product {
    String uniqueIdentifier;
    String name;
    double price;
    String type;
    String description;
    int weight;
    int quantityInStock;
  }

  enum ProductType {
    Electronics, Clothing, HomeGoods;
  }

  class Warehouse {
    String uniqueIdentifier;
    String location;
    Product [][] storedProducts;
  }

  class Customer {
    String uniqueIdentifier;
    Order [][ ] orders;
  }

  class Order {
    String uniqueIdentifier;
    double totalCost;
    Product [][ ] products;
  }

  class PaymentSystem {
    void processPayment(Customer customer, Order order);
  }

  class ShippingSystem {
    ShippingOption [ ] shippingOptions;
    void shipProduct(Product product, Customer customer);
  }

  enum ShippingOption {
    Standard, Expedited, Free;
  }

  class RatingSystem {
    void rateProduct(Product product, int rating, String timestamp);
    void rateSeller(Seller seller, int rating, String timestamp);
  }

  composition Seller -> Product [*];
  composition Seller -> Warehouse [*];
  composition Customer -> Order [*];
  composition Order -> Product [*];
  composition Warehouse -> Product [*];
}