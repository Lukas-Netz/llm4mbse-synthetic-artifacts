
classdiagram DigitalNetworkingPlatform {

  class User {
    String uniqueIdentifier;
    String name;
    String email;
  }

  class Profile {
    String uniqueIdentifier;
    User user;
    String bio;
    String skills;
    String interests;
  }

  class Group {
    String uniqueIdentifier;
    String name;
    String description;
    User [][] members;
  }

  class Content {
    String uniqueIdentifier;
    String title;
    String description;
    String topic;
  }

  class Topic {
    String uniqueIdentifier;
    String name;
    String description;
    Content [][] content;
  }

  class Webinar {
    String uniqueIdentifier;
    String title;
    String description;
    Presenter [][ ] presenters;
  }

  class Presenter {
    String uniqueIdentifier;
    String name;
  }

