
classdiagram Insurance {

  class Policy {
    String uniqueIdentifier;
    Date startDate;
    Date endDate;
    double premium;
    Insurer insurer;
    Insured insured;
  }

  class Insurer {
    String uniqueIdentifier;
    String name;
  }

  class Insured {
    String uniqueIdentifier;
    String name;
  }

  class Claim {
    String uniqueIdentifier;
    String description;
    ClaimStatus status;
    Policy policy;
    Insured insured;
  }

  class Settlement {
    String uniqueIdentifier;
    double amount;
    SettlementStatus status;
    Claim claim;
  }

  enum ClaimStatus {
    PENDING, APPROVED, REJECTED;
  }

  enum SettlementStatus {
    PENDING, APPROVED, REJECTED;
  }

  association Policy --o000o- 											;
  association Insurer --o000o- 											;
  association Insured --o000o- 											;
  association Claim --o000o- 											;
  association Settlement --o000o- 										;
}