
classdiagram WritingWorkshop {

  class Workshop {
    String name;
    String uniqueIdentifier;
    Instructor [ ] instructors;
  }

  class User {
    String username;
    String email;
    Session [ ] attendedSessions;
  }

  class Session {
    String title;
    Date date;
    User [ ] attendees;
    Discussion [ ] discussions;
  }

  class Discussion {
    String title;
    String description;
    Comment [ ] comments;
  }

  class Comment {
    String text;
    Date timestamp;
  }

  class Instructor {
    String name;
    String bio;
    Session [ ] createdSessions;
  }

