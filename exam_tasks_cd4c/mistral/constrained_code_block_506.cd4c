
classdiagram RoboticsSystem {

  class Robot {
    String name;
    String uniqueIdentifier;
    -> Sensor [*];
    void clean();
    void cook();
    void provideCompanionship();
  }

  abstract class Sensor {
    String uniqueIdentifier;
    String data;
  }

  class TemperatureSensor extends Sensor {
    int temperature;
  }

  class HumiditySensor extends Sensor {
    int humidity;
  }

  class MotionSensor extends Sensor {
    boolean isMotionDetected;
  }

  class UI {
    String name;
    String uniqueIdentifier;
    Robot robot;
    void monitor();
    void control();
  }

  class CommunicationProtocol {
    void connect(Robot robot, UI ui);
    void sendData(Sensor sensor);
  }

  composition RoboticsSystem -> (has) Robot;
  composition Robot -> Sensor [*];
  association UI -- CommunicationProtocol;
  association Robot -- CommunicationProtocol;
}