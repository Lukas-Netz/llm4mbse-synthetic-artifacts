
classdiagram RetailAnalytics {

  class RetailAnalyticsSystem {
    SalesDataProvider salesDataProvider;
    CustomerBehaviorProvider customerBehaviorProvider;
    PredictiveAnalytics predictiveAnalytics;
    BusinessIntelligenceTools biTools;
    DataVisualizationTools dvTools;
  }

  interface SalesDataProvider {
    SalesData getSalesData();
  }

  interface CustomerBehaviorProvider {
    CustomerBehavior getCustomerBehavior();
  }

  class PredictiveAnalytics {
    SalesData salesData;
    CustomerBehavior customerBehavior;
    Forecast forecast;
  }

  class SalesData {
    Product product;
    Category category;
    Store store;
    Date date;
    int quantity;
    double revenue;
  }

  enum Product {
    PRODUCT_1, PRODUCT_2, PRODUCT_3;
  }

  enum Category {
    CATEGORY_1, CATEGORY_2, CATEGORY_3;
  }

  enum Store {
    STORE_1, STORE_2, STORE_3;
  }

  class CustomerBehavior {
    Customer customer;
    Store store;
    Date date;
    int visits;
    double averageBasketValue;
  }

  class Forecast {
    Product product;
    Category category;
    Store store;
    Date date;
    int predictedSales;
  }

  class Customer {
    String id;
    String name;
    String email;
  }

  composition RetailAnalyticsSystem -> SalesDataProvider;
  composition RetailAnalyticsSystem -> CustomerBehaviorProvider;
  composition RetailAnalyticsSystem -> PredictiveAnalytics;
  composition RetailAnalyticsSystem -> BusinessIntelligenceTools;
  composition RetailAnalyticsSystem -> DataVisualizationTools;
}