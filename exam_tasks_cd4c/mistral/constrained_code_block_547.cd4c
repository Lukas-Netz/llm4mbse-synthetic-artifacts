
classdiagram MaglevTransport {

  class Station {
    String name;
    String uniqueIdentifier;
  }

  class Track {
    String type;
    Station startStation;
    Station endStation;
  }

  abstract class Train {
    String uniqueIdentifier;
    Station assignedStation;
  }

  class PassengerTrain extends Train {
    int numberOfSeats;
    int speed;
  }

  class CargoTrain extends Train {
    String cargoType;
    int weight;
  }

  class Sensor {
    Track track;
    void monitorSpeed();
    void monitorPosition();
  }

  class ControlCenter {
    void manageTrains();
    void ensureSafety();
    void optimizeEfficiency();
  }

  composition Station [*] Track [partOf]
  -- composition/ 											;
  composition Track [*] Train [assignedTo]
  -- association/ 											;
  association Sensor [*] Track [monitors]
  -- association/ 											;
  association ControlCenter [1] Track [manages]
  -- association/ 											;
  association ControlCenter [1] Train [manages]

  -- CustomTypes/ 											;
  enum TrackType {
    Straight, Curved;
  }

  enum CargoType {
    Food, Electronics, Machinery;
  }
}