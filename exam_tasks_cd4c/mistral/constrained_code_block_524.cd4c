
classdiagram RefugeeResettlement {

  class Refugee {
    String uniqueIdentifier;
    String name;
    String dateOfBirth;
    String countryOfOrigin;
    String preferredDestination;
  }

  class Application {
    Refugee refugee;
    List<String> preferredDestinations;
  }

  class DestinationCountry {
    String uniqueIdentifier;
    String name;
    List<HousingOption> housingOptions;
  }

  class HousingOption {
    String uniqueIdentifier;
    String name;
    List<Room> rooms;
  }

  class Room {
    String uniqueIdentifier;
    String name;
    List<Furniture> furniture;
  }

  class Furniture {
    String uniqueIdentifier;
    String name;
    List<Size> sizes;
  }

  class Size {
    String uniqueIdentifier;
    String name;
    List<Material> materials;
  }

  class Material {
    String uniqueIdentifier;
    String name;
    List<Color> colors;
  }

  class Color {
    String uniqueIdentifier;
    String name;
    List<Shade> shades;
  }

  class Shade {
    String uniqueIdentifier;
    String name;
  }

  association Refugee --oApplication;
  association Application --oDestinationCountry;
  association DestinationCountry --oHousingOption [*];
  association HousingOption --oRoom [*];
  association Room --oFurniture [*];
  association Furniture --oSize [*];
  association Size --oMaterial [*];
  association Material --oColor [*];
  association Color --oShade [*];
}