



classdiagram MentalHealthPlatform  {

  class User  {
    String username;
    String password;
    Profile profile;
    SessionHistory sessionHistory;
    RatingAndReview ratingAndReview;
  }

  class Therapist  {
    String username;
    String password;
    Profile profile;
    Schedule schedule;
    SessionHistory sessionHistory;
    RatingAndReview ratingAndReview;
  }

  class Session  {
    Date date;
    Time time;
    Therapist therapist;
    User user;
    SessionType type;
  }

  class SupportGroup  {
    String name;
    Therapist therapist;
    User[] users;
  }

  class Profile  {
    String name;
    String email;
    String language;
    String currency;
  }

  class SessionHistory  {
    Session[] sessions;
  }

  class RatingAndReview  {
    int rating;
    String review;
  }

  class Schedule  {
    Date[] dates;
    Time[] times;
  }

  class SessionType  {
    String type;
  }

  class ProfileLanguage  {
    String language;
  }

  class ProfileCurrency  {
    String currency;
  }

  composition User -> Profile;
  composition User -> SessionHistory;
  composition User -> RatingAndReview;
  composition Therapist -> Profile;
  composition Therapist -> Schedule;
  composition Therapist -> SessionHistory;
  composition Therapist -> RatingAndReview;
  composition Session -> Therapist;
  composition Session -> User;
  

}