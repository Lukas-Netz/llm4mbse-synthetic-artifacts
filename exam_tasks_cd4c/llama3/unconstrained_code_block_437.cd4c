



classdiagram VirtualRealityTourismPlatform  {

  class Destination  {
    String name;
    String description;
    List<Attraction> attractions;
  }

  class Attraction  {
    String name;
    String description;
    AttractionType type;
  }

  class User  {
    String username;
    String password;
    List<Destination> wishlist;
    List<Review> reviews;
  }

  class Review  {
    String text;
    Attraction attraction;
  }

  class Administrator  {
    List<Destination> managedDestinations;
    List<Attraction> managedAttractions;
    List<Review> managedReviews;
  }

  enum AttractionType  {
    Museum, Landmark, Park;
  }

  composition Destination -> Attraction[*];
  composition User -> Review[*];
  composition Administrator -> Destination[*];
  composition Administrator -> Attraction[*];
  composition Administrator -> Review[*];
}
