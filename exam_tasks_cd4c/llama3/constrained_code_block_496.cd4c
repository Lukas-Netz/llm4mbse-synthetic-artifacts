



classdiagram OnlinePlatform  {

  class Course  {
    String title;
    String description;
    String uniqueIdentifier;
    Level level;
  }

  class Student  {
    String uniqueIdentifier;
    Role role;
  }

  class Assignment  {
    String title;
    String description;
    Date dueDate;
    Grade grade;
  }

  class DiscussionTopic  {
    String title;
    String description;
  }

  class Instructor extends Student  {
    void createCourse(Course course);
    void manageCourse(Course course);
    void viewStudentProgress(Student student);
  }

  class TeachingAssistant extends Student  {
    void assistInstructor(Instructor instructor);
  }

  class Level  {
    String name;
  }

  class Role  {
    String name;
  }

  class Grade  {
    double score;
  }

  class Date  {
    int day;
    int month;
    int year;
  }

  composition Course -> (has) Level;
  composition Student -> (has) Role;
  composition Assignment -> (has) Grade;
  composition DiscussionTopic -> (has) Student;
  composition Instructor -> (has) Course;
  composition TeachingAssistant -> (has) Instructor;
  composition OnlinePlatform -> (has) Course [1..*];
  composition OnlinePlatform -> (has) Student [1..*];
  composition OnlinePlatform -> (has) Assignment [1..*];
  composition OnlinePlatform -> (has) DiscussionTopic [1..*];
}