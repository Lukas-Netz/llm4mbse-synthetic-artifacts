



classdiagram CloudService  {

  class Service  {
    String uniqueIdentifier;
    String name;
    Set<Feature> features;
    PricingPlan pricingPlan;
  }

  class StorageService extends Service  {
    String storageType;
  }

  class ProcessingService extends Service  {
    String processingType;
  }

  class NetworkingService extends Service  {
    String networkingType;
  }

  class Feature  {
    String name;
    String description;
  }

  class PricingPlan  {
    double basePrice;
    double usageBasedPrice;
  }

  class LoyaltyProgram  {
    Set<Rule> rules;
    RewardSystem rewardSystem;
  }

  class Rule  {
    String condition;
    String reward;
  }

  class RewardSystem  {
    Set<Reward> rewards;
  }

  class Reward  {
    String description;
    double value;
  }

  class CustomerSupportTeam  {
    Set<Skill> skills;
    Set<KnowledgeBase> knowledgeBases;
  }

  class Skill  {
    String name;
    String description;
  }

  class KnowledgeBase  {
    String title;
    String description;
  }

  class Policy  {
    String name;
    String description;
  }

  composition Service -> Feature [0..*];
  composition Service -> PricingPlan;
  composition LoyaltyProgram -> Rule [0..*];
  composition CustomerSupportTeam -> Skill [0..*];
  composition CustomerSupportTeam -> KnowledgeBase [0..*];
  composition CloudService -> Service [0..*];
  }