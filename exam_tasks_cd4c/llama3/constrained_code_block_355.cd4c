



classdiagram DevOpsToolchain  {

  class Project  {
    String name;
    String description;
    List<Task> tasks;
  }

  class Task  {
    String name;
    String description;
    TaskStatus status;
    TeamMember assignedTo;
  }

  class TeamMember  {
    String name;
    String email;
    List<Project> projects;
  }

  class VersionControlSystem  {
    List<Repository> repositories;
  }

  class Repository  {
    String name;
    List<File> files;
  }

  class File  {
    String name;
    String content;
  }

  class CICDSystem  {
    BuildTool buildTool;
    TestingFramework testingFramework;
  }

  class BuildTool  {
    String name;
  }

  class TestingFramework  {
    String name;
  }

  enum TaskStatus  {
    ToDo, InProgress, Done;
  }

  composition Project -> (has) Task[*];
  composition Task -> (has) TeamMember [0..1];
  composition TeamMember -> (has) Project[*];
  composition VersionControlSystem -> (has) Repository[*];
  composition Repository -> (has) File[*];
  composition CICDSystem -> (uses) BuildTool;
  composition CICDSystem -> (uses) TestingFramework;
}