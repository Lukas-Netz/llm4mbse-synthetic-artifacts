



classdiagram ConversationalInterface  {

  class User  {
    String name;
    boolean isHuman;
  }

  class Query  {
    String text;
    VoiceCommand voiceCommand;
  }

  class VoiceCommand  {
    String command;
  }

  class Response  {
    String text;
    Image image;
    Audio audio;
  }

  class KnowledgeBase  {
    Topic  topic;
  }

  class Topic  {
    String title;
    String description;
  }

  class ExternalAPI  {
    String url;
  }

