    Response response;
  }

  class Device  {
    String name;
  }

  class SmartHome  {
    Device  device;
  }

  class Interface  {
    User user;
    Query query;
    Response response;
    KnowledgeBase knowledgeBase;
    ExternalAPI externalAPI;
    Suggestion suggestion;
    Feedback feedback;
    SmartHome smartHome;
  }

  composition Interface  ->  (has) User;
