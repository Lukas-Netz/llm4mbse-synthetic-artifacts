



classdiagram IntellectualProperty  {

  class Creator  {
    String name;
    String email;
  }

  class Owner  {
    String name;
    String email;
  }

  class User  {
    String name;
    String email;
  }

  class Authority  {
    String name;
    String email;
  }

  class IntellectualProperty  {
    String title;
    Creator creator;
    Owner owner;
    User user;
    Authority authority;
    boolean isRegistered;
    boolean isTransferred;
  }

  class Patent extends IntellectualProperty  {
    String patentNumber;
  }

  class Trademark extends IntellectualProperty  {
    String trademarkName;
  }

  class Copyright extends IntellectualProperty  {
    String copyrightText;
  }

  class Registration  {
    IntellectualProperty intellectualProperty;
    Authority authority;
    Date registrationDate;
  }

  class Transfer  {
    IntellectualProperty intellectualProperty;
    Owner oldOwner;
    Owner newOwner;
    Date transferDate;
  }

  composition IntellectualProperty -> (has) Creator;
  composition IntellectualProperty -> (has) Owner;
  composition IntellectualProperty -> (has) User;
  composition IntellectualProperty -> (has) Authority;
  composition IntellectualProperty -> (has) Registration [0..1];
  composition IntellectualProperty -> (has) Transfer [0..1];
  composition Registration -> (has) IntellectualProperty;
  composition Transfer -> (has) IntellectualProperty;
  composition Patent -> (has) Registration [0..1];
  composition Trademark -> (has) Registration [0..1];
  }