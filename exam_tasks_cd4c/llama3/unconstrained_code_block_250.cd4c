



classdiagram OnlinePhilosophyCourses  {

  class Instructor  {
    String name;
    String email;
    -> Course[*];
  }

  class Course  {
    String name;
    String description;
    -> Instructor[*];
    -> Student[*];
    Date startDate;
    Date endDate;
  }

  class Student  {
    String name;
    String email;
    -> Course[*];
  }

  composition Instructor -> Course[*];
  composition Course -> Instructor[*];
  composition Course -> Student[*];
  composition Student -> Course[*];
}
