



classdiagram PortfolioSystem  {

  class Portfolio  {
    User user;
    Project[] projects;
    Skill[] skills;
    Achievement[] achievements;
  }

  class User  {
    String username;
    Portfolio portfolio;
  }

  class Project  {
    String title;
    String description;
    String githubRepositoryLink;
  }

  class Skill  {
    String name;
    Category category;
  }

  enum Category  {
    ProgrammingLanguage,
    Framework,
    Tool
  ;}

  class Achievement  {
    String title;
    String description;
  }

  class PortfolioSystem  {
    User[] users;
  }

  composition Portfolio  ->  (has) User;
  composition Portfolio  ->  (has) Project[*];
  composition Portfolio  ->  (has) Skill[*];
  composition Portfolio  ->  (has) Achievement[*];
  composition PortfolioSystem  ->  (has) User[*];
  association User  ->  (has) Portfolio;
  association Project  ->  (has) Portfolio;
  association Skill  ->  (has) Portfolio;
  association Achievement  ->  (has) Portfolio;
}