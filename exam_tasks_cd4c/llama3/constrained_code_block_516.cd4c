



classdiagram HomeImprovementPlatform  {

  class User  {
    String name;
    String email;
    String password;
  }

  class Project  {
    String name;
    String description;
    double budget;
    User owner;
  }

  class Contractor  {
    String name;
    String contactInformation;
    double rating;
  }

  class Task  {
    String name;
    String description;
    Date dueDate;
    Contractor assignedContractor;
    Project project;
  }

  class Budget  {
    double totalBudget;
    double expenses;
  }

  class ProjectBudget  {
    Project project;
    Budget budget;
  }

  class TaskAssignment  {
    Task task;
    Contractor contractor;
  }

  class ProjectSharing  {
    Project project;
    User sharedUser;
  }

  class Review  {
    Task task;
    Contractor contractor;
    double rating;
    String comment;
  }

  composition User -> (has) Project[*];
  composition Project -> (has) Task[*];
  composition Project -> (has) Budget;
  composition Task -> (has) Contractor;
  composition Budget -> (has) Project;
  composition ProjectSharing -> (has) Project;
  composition Review -> (has) Task;
  composition Review -> (has) Contractor;
}