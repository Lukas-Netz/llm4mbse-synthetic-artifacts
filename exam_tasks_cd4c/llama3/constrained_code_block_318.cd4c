



classdiagram InventoryControlSystem  {

  class Product  {
    String uniqueId;
    String name;
    String description;
    ProductType type;
  }

  class Location  {
    String uniqueId;
    String name;
    String address;
    LocationType type;
  }

  class InventoryItem  {
    String uniqueId;
    Product product;
    Location location;
    int quantity;
  }

  class Order  {
    String uniqueId;
    Product product;
    int quantity;
  }

  class Report  {
    String type;
    String data;
  }

  enum ProductType  {
    Electronics, Clothing, etc;
  }

  enum LocationType  {
    Warehouse, Store, etc;
  }

  composition Location -> InventoryItem [*];
  composition Product -> InventoryItem [*];
  composition Order -> Product [*];
  composition Report -> InventoryItem [*];
  composition Report -> Order [*];
}