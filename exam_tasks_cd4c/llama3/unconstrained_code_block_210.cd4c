



classdiagram DancePlatform  {

  class User  {
    String username;
    String password;
  }

  class Instructor extends User  {
    String danceStyleSpecialty;
    String bio;
  }

  class Student extends User  {
    String danceExperience;
    String danceGoals;
  }

  class Class  {
    String name;
    String description;
    Schedule schedule;
  }

  class Schedule  {
    Date startTime;
    Date endTime;
    Frequency frequency;
  }

  enum Frequency  {
    Weekly, Biweekly, Monthly;
  }

  class Review  {
    String comment;
    int rating;
  }

  class Rating  {
    int rating;
  }

  composition Instructor -> Class [1..*];
  composition Student -> Class [1..*];
  composition Class -> Review [0..*];
  composition Instructor -> Rating [0..*];
  composition Student -> Rating [0..*];
}

