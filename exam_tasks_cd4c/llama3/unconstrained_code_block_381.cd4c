



classdiagram SimulationCenter  {

  class Room  {
    Equipment equipment;
    Mannequin mannequin;
  }

  class Instructor  {
    AreaOfExpertise expertise;
    Room room;
  }

  class Mannequin  {
    Condition condition;
  }

  class Equipment  {
    String type;
  }

  class AreaOfExpertise  {
    String name;
  }

  class Condition  {
    String description;
  }

  class SchedulingSystem  {
    Room[] rooms;
    Instructor[] instructors;
    Mannequin[] mannequins;
    Equipment[] equipment;
  }

  composition SchedulingSystem -> (has) Room[*];
  composition SchedulingSystem -> (has) Instructor[*];
  composition SchedulingSystem -> (has) Mannequin[*];
  composition SchedulingSystem -> (has) Equipment[*];
  composition Room -> (has) Equipment;
  composition Room -> (has) Mannequin;
  composition Instructor -> (has) Room;
  composition Mannequin -> (has) Condition;
}
