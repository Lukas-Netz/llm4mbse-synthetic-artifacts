



classdiagram CalligraphyCoursePlatform  {

  class Course  {
    String title;
    String description;
    String uniqueIdentifier;
    Level level;
  }

  class User  {
    String uniqueIdentifier;
    Role role;
  }

  class Instructor extends User  {
    Course[] coursesCreated;
  }

  class Student extends User  {
    Course[] coursesEnrolled;
  }

  class Lesson  {
    Course course;
    Material[] materials;
  }

  class Material  {
    String type;
    String content;
  }

  class Review  {
    Course course;
    User reviewer;
    int rating;
    String comment;
  }

  class Enrollment  {
    Course course;
    User user;
    Date enrollmentDate;
    Date completionDate;
  }

  enum Level  {
    Beginner, Intermediate, Advanced;
  }

  enum Role  {
    Instructor, Student;
  }

  composition Course -> (has) Level;
  composition User -> (has) Role;
  composition Instructor -> (has) Course[*];
  composition Student -> (has) Course[*];
  composition Lesson -> (has) Course;
  composition Lesson -> (has) Material[*];
  composition Material -> (has) String;
  composition Review -> (has) Course;
  composition Review -> (has) User;
  composition Enrollment -> (has) Course;
  }