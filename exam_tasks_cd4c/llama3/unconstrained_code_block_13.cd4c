    List<Report> reports;
  }

  composition AnalyticsSystem -> (has) Sport[*];
  composition AnalyticsSystem -> (has) Team[*];
  composition AnalyticsSystem -> (has) Player[*];
  composition AnalyticsSystem -> (has) Game[*];
  composition AnalyticsSystem -> (has) Statistic[*];
  composition AnalyticsSystem -> (has) Report[*];
  composition Sport -> (has) Team[*];
  composition Team -> (has) Player[*];
  composition Game -> (has) Team[*];
  composition Statistic -> (has) Game[*];
  composition Report -> (has) Statistic[*];
}

