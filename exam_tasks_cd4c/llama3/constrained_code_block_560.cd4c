



classdiagram GenomeEditingSoftware  {

  class Project  {
    String name;
    String description;
    List<Gene> genesToEdit;
  }

  class Gene  {
    String name;
    String sequence;
    GeneStatus status;
    List<EditHistory> editHistory;
  }

  class User  {
    String name;
    String email;
    UserRole role;
    List<Project> assignedProjects;
  }

  enum UserRole  {
    Administrator,
    Editor
  ;}

  enum GeneStatus  {
    Unedited,
    Edited
  ;}

  class EditHistory  {
    Date timestamp;
    String description;
  }

  composition Project -> (has) Gene[*];
  composition User -> (has) Project[*];
  composition Gene -> (has) EditHistory[*];
  association User -> (assigns) Project[*];
  association Project -> (has) User[*];
  association Gene -> (has) User[*];
}