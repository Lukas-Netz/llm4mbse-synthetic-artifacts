



classdiagram MusicProductionCoursePlatform  {

  class Course  {
    String title;
    String description;
    String uniqueIdentifier;
    Genre genre;
  }

  class Student  {
    String name;
    String email;
    String uniqueIdentifier;
  }

  class Instructor  {
    String name;
    String uniqueIdentifier;
  }

  class Lesson  {
    String title;
    String description;
    String uniqueIdentifier;
    Topic topic;
  }

  class Enrollment  {
    Date startDate;
    Date endDate;
    Course course;
    Student student;
  }

  class Administrator  {
    void manageCourse(Course course);
    void manageInstructor(Instructor instructor);
    void manageStudent(Student student);
    void trackStudentProgress(Student student);
    void trackCourseRatings(Course course);
  }

  enum Genre  {
    Electronic, Rock, Pop;
  }

  enum Topic  {
    Recording, Mixing, Mastering;
  }

  composition Course -> (has) Instructor [0..*];
  composition Course -> (has) Lesson [0..*];
  composition Student -> (has) Enrollment [0..*];
  composition Administrator -> (has) Course [0..*];
  composition Administrator -> (has) Instructor [0..*];
  composition Administrator -> (has) Student [0..*];
}
