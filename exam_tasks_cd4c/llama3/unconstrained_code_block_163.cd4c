



classdiagram FitnessTracker  {

  class FitnessTrackerDevice  {
    String uniqueIdentifier;
    UserAccount userAccount;
  }

  class UserAccount  {
    String name;
    String email;
  }

  class Activity  {
    String type;
    int duration;
    int distance;
    int caloriesBurned;
  }

  class HeartRateData  {
    int heartRate;
  }

  class BloodPressureData  {
    int systolic;
    int diastolic;
  }

  class SleepData  {
    int sleepQuality;
  }

  class FitnessGoal  {
    String goalType;
    int targetValue;
  }

  class SmartphoneApp  {
    FitnessTrackerDevice device;
  }

  composition FitnessTrackerDevice -> UserAccount;
  composition FitnessTrackerDevice -> Activity[*];
  composition FitnessTrackerDevice -> HeartRateData;
  composition FitnessTrackerDevice -> BloodPressureData;
  composition FitnessTrackerDevice -> SleepData;
  composition FitnessTrackerDevice -> FitnessGoal;
  composition SmartphoneApp -> FitnessTrackerDevice;
}
