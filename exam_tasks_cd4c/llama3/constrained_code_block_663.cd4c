



classdiagram VirtualTourSystem  {

  class Property  {
    String name;
    String address;
    VirtualTour[] virtualTours;
  }

  class VirtualTour  {
    String name;
    String description;
    Scene[] scenes;
  }

  class Scene  {
    String name;
    String description;
    Model3D model;
  }

  abstract class Model3D  {
    String type;
  }

  class Room extends Model3D  {
    double length;
    double width;
    double height;
  }

  class Corridor extends Model3D  {
    double length;
    double width;
  }

  class Exterior extends Model3D  {
    double latitude;
    double longitude;
    double altitude;
  }

  class User  {
    void createVirtualTour(VirtualTour virtualTour);
    void editVirtualTour(VirtualTour virtualTour);
    void deleteVirtualTour(VirtualTour virtualTour);
    void uploadModel(Model3D model);
    void downloadModel(Model3D model);
    VirtualTour[] searchVirtualTours(String query);
  }

  composition Property -> VirtualTour[*];
  composition VirtualTour -> Scene[*];
  composition Scene -> Model3D;
  association User -> VirtualTour[*];
  association User -> Model3D[*];
}