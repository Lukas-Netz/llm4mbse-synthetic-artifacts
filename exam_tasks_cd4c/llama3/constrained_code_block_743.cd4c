



classdiagram DigitalArtSystem  {

  class Artist  {
    String uniqueIdentifier;
    String name;
  }

  class Artwork  {
    String uniqueIdentifier;
    String title;
    String description;
    Artist artist;
  }

  class Collector  {
    String uniqueIdentifier;
    String name;
  }

  class Transaction  {
    Collector collector;
    Artist artist;
    Artwork artwork;
    double amount;
  }

  class Blockchain  {
    List<Transaction> transactions;
  }

  composition Blockchain  ->  (stores) Transaction[*];
  composition Artwork  ->  (created_by) Artist;
  composition Transaction  ->  (involves) Collector;
  composition Transaction  ->  (involves) Artist;
  composition Transaction  ->  (involves) Artwork;
  composition Artwork  ->  (owned_by) Collector [0..1];
}