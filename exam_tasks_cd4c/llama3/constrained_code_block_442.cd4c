



classdiagram GardeningAdvice  {

  class User  {
    String username;
    String password;
  }

  class Question  {
    int id;
    String title;
    String description;
    String[] tags;
  }

  class Answer  {
    int id;
    String text;
    int rating;
  }

  class Expert  {
    int id;
    String name;
    String category;
  }

  class GardeningAdviceSystem  {
    User[] users;
    Question[] questions;
    Answer[] answers;
    Expert[] experts;
  }

  composition GardeningAdviceSystem -> User[*];
  composition GardeningAdviceSystem -> Question[*];
  composition GardeningAdviceSystem -> Answer[*];
  composition GardeningAdviceSystem -> Expert[*];

  association User -> Question[*];
  association Question -> Answer[*];
  association Expert -> Question[*];
  association Answer -> User[*];
  association Expert -> GardeningAdviceSystem;
}