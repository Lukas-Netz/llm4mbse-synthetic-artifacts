



classdiagram CarbonNanotubeProductionSystem  {

  class Reactor  {
    String uniqueIdentifier;
    int productionCapacityGph;
  }

  class ControlUnit  {
    -> Reactor[*];
    void adjustProductionRate(Reactor reactor);
    void updateInventory(CarbonNanotube carbonNanotube);
  }

  class CarbonNanotube  {
    String type;
    int quantity;
  }

  class QualityControl  {
    -> CarbonNanotube[*];
    boolean test(CarbonNanotube carbonNanotube);
    void reject(CarbonNanotube carbonNanotube);
  }

  class ShippingDepartment  {
    -> CarbonNanotube[*];
    void packageAndShip(CarbonNanotube carbonNanotube, Customer customer);
  }

  class Customer  {
    String name;
  }

  composition ControlUnit -> Reactor[*];
  composition QualityControl -> CarbonNanotube[*];
  composition ShippingDepartment -> CarbonNanotube[*];
  composition ShippingDepartment -> Customer[*];
}