



classdiagram SmartFarm  {

  class Sensor  {
    Temperature temperature;
    Humidity humidity;
    Movement movement;
  }

  class Camera  {
    Image image;
  }

  class CentralHub  {
    SensorData sensorData;
    CameraData cameraData;
    Analysis analysis;
    Report report;
    Recommendation recommendation;
  }

  class Animal  {
    Breed breed;
    Age age;
    HealthStatus healthStatus;
  }

