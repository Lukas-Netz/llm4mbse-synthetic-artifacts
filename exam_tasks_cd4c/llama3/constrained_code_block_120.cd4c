



classdiagram DigitalAdvertisingPlatform  {

  class AdCampaign  {
    String uniqueIdentifier;
    String name;
    AdType type;
    Budget budget;
    TargetAudience targetAudience;
  }

  enum AdType  {
    DisplayAd,
    VideoAd,
    SponsoredContent
  ;}

  class DisplayAd extends AdCampaign  {
    String image;
    String headline;
    String description;
  }

  class VideoAd extends AdCampaign  {
    String videoUrl;
    String title;
    String description;
  }

  class SponsoredContent extends AdCampaign  {
    String text;
    String image;
    String link;
  }

  class Budget  {
    double amount;
  }

  class TargetAudience  {
    String demographic;
    String interests;
  }

  class AdPerformanceMetrics  {
    int impressions;
    int clicks;
    int conversions;
    double cost;
  }

  class AdInsights  {
    AdRotation adRotation;
    AdScheduling adScheduling;
    AdTargeting adTargeting;
  }

  class AdRotation  {
    String ad1;
    String ad2;
    String ad3;
  }

  class AdScheduling  {
    String startDateTime;
    String endDateTime;
  }

  class AdTargeting  {
    String demographic;
    String interests;
  }

  composition AdCampaign -> (has) Budget;
  composition AdCampaign -> (has) TargetAudience;
  composition AdCampaign -> (has) AdPerformanceMetrics;
  composition AdCampaign -> (has) AdInsights;
  composition AdInsights -> (has) AdRotation;
  composition AdInsights -> (has) AdScheduling;
  composition AdInsights -> (has) AdTargeting;
}