



classdiagram PredictiveDemandForecasting  {

  class Product  {
    String uniqueIdentifier;
    String name;
  }

  class Warehouse  {
    String uniqueIdentifier;
    String name;
  }

  class ProductWarehouse  {
    Product product;
    Warehouse warehouse;
  }

  class DemandForecast  {
    Product product;
    Warehouse warehouse;
    int demand;
  }

  class HistoricalSalesData  {
    Product product;
    Warehouse warehouse;
    Date date;
    int quantity;
  }

  class ForecastingModel  {
    String type;
    double alpha;
    double beta;
  }

  class Administrator  {
    void inputHistoricalSalesData(HistoricalSalesData data);
    void configureForecastingModel(ForecastingModel model);
    void viewPredictedDemand(DemandForecast demand);
  }

  composition Warehouse -> ProductWarehouse [*];
  composition Product -> ProductWarehouse [*];
  composition Administrator -> ForecastingModel [1];
  composition ForecastingModel -> HistoricalSalesData [*];
  composition DemandForecast -> Product [1];
  composition DemandForecast -> Warehouse [1];
}