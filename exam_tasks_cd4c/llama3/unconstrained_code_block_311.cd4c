



classdiagram MaglevTransportationSystem  {

  class Station  {
    String name;
    String uniqueIdentifier;
  }

  class Track  {
    Station from;
    Station to;
    boolean isStraight;
  }

  abstract class Train  {
    String uniqueIdentifier;
    int capacity;
  }

  class PassengerTrain extends Train  {
    int numSeats;
    int speed;
  }

  class CargoTrain extends Train  {
    String cargoType;
    int weight;
  }

  class Sensor  {
    Track track;
    double speed;
    double position;
  }

  class ControlCenter  {
    void manageTrains();
  }

  composition Station -> Track[*];
  composition Track -> Sensor[*];
  composition ControlCenter -> Train[*];
}

