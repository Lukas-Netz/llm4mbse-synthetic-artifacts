



classdiagram HealthInformationExchange  {

  class Patient  {
    String uniqueIdentifier;
    String name;
    Date dateOfBirth;
    String address;
    MedicalHistory medicalHistory;
    TestResults testResults;
    MedicationList medicationList;
  }

  class MedicalHistory  {
    List<Diagnosis> diagnoses;
    List<Allergy> allergies;
    List<Medication> medications;
  }

  class TestResults  {
    List<LabResult> labResults;
    List<ImagingResult> imagingResults;
    List<DiagnosticTest> diagnosticTests;
  }

  class MedicationList  {
    List<Prescription> prescriptions;
  }

  class HealthcareProvider  {
    String uniqueIdentifier;
    String name;
    String credentials;
  }

  class Request  {
    Patient patient;
    HealthcareProvider provider;
    DataRequest dataRequest;
  }

  class DataRequest  {
    String dataType;
  }

  class Diagnosis  {
    String name;
    Date date;
  }

  class Allergy  {
    String name;
    Date date;
  }

  class Medication  {
    String name;
    String dosage;
  }

  class LabResult  {
    String testName;
    String result;
  }

  class ImagingResult  {
    String testName;
    String result;
  }

  class DiagnosticTest  {
    String testName;
    String result;
  }

  class Prescription  {
    Medication medication;
    Dosage dosage;
  }

  class Dosage  {
    String amount;
    String frequency;
  }

  composition Patient -> (has) MedicalHistory;
  composition Patient -> (has) TestResults;
  composition Patient -> (has) MedicationList;
  composition HealthcareProvider -> (requests) Patient;
  composition Request -> (has) DataRequest;
  composition DataRequest -> (has) Patient;
  composition DataRequest -> (has) HealthcareProvider;
}
